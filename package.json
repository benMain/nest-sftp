{
  "name": "nest-sftp",
  "author": "Benjamin Main",
  "description": "Nest Framework wrapper around ssh2-sftp-client",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nest",
    "sftp",
    "lambda"
  ],
  "homepage": "https://github.com/benMain/nest-sftp",
  "bugs": {
    "url": "https://github.com/benMain/nest-sftp/issues"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/benMain/nest-sftp.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/core": "^7.0.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "ssh2-sftp-client": "^9.0.4"
  },
  "devDependencies": {
    "@nestjs/testing": "7.0.7",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.2.2",
    "@types/node": "^14.18.10",
    "@types/ssh2-sftp-client": "^9.0.0",
    "@types/supertest": "2.0.8",
    "husky": "^4.2.3",
    "import-sort-style-eslint": "^6.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^10.1.1",
    "prettier": "2.0.2",
    "prettier-plugin-import-sort": "0.0.4",
    "semantic-release": "^19.0.5",
    "supertest": "4.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "8.8.1",
    "tsc-watch": "4.2.3",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "release": {
    "branch": "master",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "breaking",
              "release": "major"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true,
          "tarballDir": "dist"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": "dist/*.tgz"
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "tslint -p tsconfig.json -c tslint.json",
      "jest --findRelatedTests"
    ],
    "./**/*.{js,json,css,md}": [
      "prettier --write \"src/**/*.ts\"",
      "git add"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "parser": "typescript"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "version": "1.1.8"
}
